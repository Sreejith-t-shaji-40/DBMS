   create table student_sreejith(rollno integer primary key,name varchar(20),dob date, dept varchar(5),marks float);

A)  SQL> insert into student_sreejith values(1,'amitha','18-jun-1988','CS',480);

    1 row created.

    SQL>  insert into student_sreejith values(2,'arya','12-jul-1989','IT',500);

